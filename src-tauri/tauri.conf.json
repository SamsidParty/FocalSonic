{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Aonsoku",
  "mainBinaryName": "Aonsoku",
  "version": "0.5.0",
  "identifier": "com.victoralvesf.aonsoku",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Aonsoku",
        "label": "main",
        "width": 1280,
        "minWidth": 1280,
        "height": 720,
        "minHeight": 720,
        "maximized": true,
        "useHttpsScheme": false,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "backgroundColor": "#0f1729",
        "theme": "Dark"
      }
    ]
  }
}